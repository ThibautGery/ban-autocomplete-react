{
  "name": "ban-autocomplete-react",
  "version": "0.0.1-alpha1",
  "description": "Autocomplete address widget that uses the French BAN database.",
  "main": "src/client/app/AutocompleteBan.jsx",
  "dependencies": {
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "react": "15.0.1",
    "react-dom": "15.0.1",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "babel-jest": "11.0.2",
    "chromedriver": "2.21.2",
    "jest": "0.1.40",
    "jest-cli": "11.0.2",
    "nightwatch": "0.8.18",
    "react-addons-test-utils": "15.0.1",
    "selenium-server": "2.53.0",
    "webpack-dev-server": "1.14.1"
  },
  "scripts": {
    "test-watch": "jest --watch",
    "test": "jest --coverage",
    "test-ci": "jest --json --coverage",
    "test-integration": "nightwatch -e chrome",
    "build": "webpack -p",
    "dev": "node node_modules/.bin/webpack-dev-server --content-base src/client/"
